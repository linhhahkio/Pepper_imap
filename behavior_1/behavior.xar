        # Get name of user
        p = self.memory.getData("logined")
        p = "".join(p)
        p = p.lower()

        # create a database connection
        conn = self.create_connection(database)
        with conn:
            p = p.decode('utf-8')
            sr = self.select_receiver_by_name(conn,p)
            self.logger.info(str(sr[0]))
            self.memory.insertData("user_id", str(sr[0]))
            self.memory.insertData("user_pass", str(sr[1]))
            self.onStopped()


    def onInput_onStop(self):
