<?xml version="1.0" encoding="UTF-8" ?><ChoregrapheProject xmlns="http://www.aldebaran-robotics.com/schema/choregraphe/project.xsd" xar_version="3"><Box name="root" id="-1" localization="8" tooltip="Root box of Choregraphe&apos;s behavior. Highest level possible." x="0" y="0"><bitmap>media/images/box/root.png</bitmap><script language="4"><content><![CDATA[]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="4" /><Timeline enable="0"><BehaviorLayer name="behavior_layer1"><BehaviorKeyframe name="keyframe1" index="1"><Diagram scale="50"><Box name="Check mail" id="1" localization="8" tooltip="Pepper checks the inbox-tab of Gmail and returns and outputs a string containing information of the messages. The outputs respond to the number of emails received.&#x0A;&#x0A;The scenario is that there is at least one new message in the inbox." x="378" y="233"><bitmap>media/images/box/box-python-script.png</bitmap><script language="4"><content><![CDATA[import imaplib
import email

class MyClass(GeneratedClass):

    mails = []
    counter = 0

    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.isRunning = False

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        try :
          self.mail.logout()
          self.memory = None
        except :
          pass

    def mail_init(self):
        mail = imaplib.IMAP4_SSL('imap.gmail.com')

        # Get id and pass
        human_id = self.memory.getData("user_id")
        human_pass = self.memory.getData("user_pass")

        mail.login(human_id, human_pass)

        mail.list()
        mail.select('INBOX')
        self.mails[:] = []
        return mail

    def check_mail(self, mail, tag):
        # define default type of mail
        typ, data = mail.uid('search', None, tag) # search and return uids instead
        unread_msg_nums = data[0].split()

        num_mail = len(unread_msg_nums)
        # iterate through uid of unseen messages

        if(num_mail != 0):
            for e_id in reversed(unread_msg_nums):
                #Get subject
                _, data = mail.uid('fetch', e_id, '(BODY[HEADER.FIELDS (SUBJECT)])' )
                info = data[0][1]

                # Remove the "Subject: " in the string
                varSubject = info.split(': ',1)[1]
                # Remove any Tab, linebreak
                varSubject = varSubject.replace("\r", "")
                varSubject = varSubject.replace("\n", "")
                varSubject = varSubject.replace("\t", "")
                # Decode non-ascii character
                varSubject = email.Header.decode_header(varSubject)[0]
                varSubject = str(varSubject[0])

                #Get sender
                _, data = mail.uid('fetch', e_id, '(BODY[HEADER.FIELDS (FROM)])' )
                info = data[0][1]
                # Remove the "From: " in the string
                varFrom = info.split(': ')[1]
                # Remove the email address
                varFrom = varFrom.split('<')[0]

                # Copy the mails to the list
                temp_mails = [str(e_id),str(varSubject),str(varFrom)]
                #self.logger.info(temp_mails)
                self.mails.append(temp_mails)

        if (tag == 'UNSEEN'):
            # Mark them as seen
            for e_id in unread_msg_nums:
                mail.uid('STORE', e_id, '-FLAGS', '\SEEN')

            # Return number of new mails
            if num_mail != 0:
                if num_mail == 1:
                    on_mail = "Sinulle on tullut " + str(num_mail) + " uusi viesti. \pau=500\ Katsotaanko keneltä se on?"
                else:
                    on_mail = "Sinulle on tullut " + str(num_mail) + " uutta viestiä. \pau=500\ Luetaanko ne?"

                self.onStopped(on_mail)
            else:
                on_mail = "Sinulle ei ole tullut uusia viestejä viime näkemän jälkeen. \pau=500\ Luetaanko aiemmin saapuneita viestejä?"
                self.mails[:] = []
                self.onNoMail(on_mail)

    def onInput_onStart(self):
        # Initiate mail account and mail box
        mail = self.mail_init()

        # Get subject and sender of each mail -------------------------#
        # Return number of new mail
        self.check_mail(mail, "UNSEEN")

        mail.logout()

    def onInput_mail_to_read(self):
        num_mails = len(self.mails)
        mail_no = self.counter % num_mails
        sentence = "Viestin lähettäjä on " + self.mails[mail_no][2] + "\\pau=500\\ Viestin otsikko on \pau=500\ " + self.mails[mail_no][1] + "\\pau=500\\ Luetaanko tämä viesti?"
        self.memory.insertData('current_mail_id', self.mails[mail_no][0])
        self.counter += 1
        self.out_mail_to_read(sentence)

    def onInput_reset_Counter(self):
        self.counter = 0

    def onInput_check_all(self):
        #Initialize mail server, user account and mail list
        mail = self.mail_init()

        # Get subject and sender of each mail -------------------------#
        self.check_mail(mail, "ALL")
        self.onInput_mail_to_read()

        mail.logout()

    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="mail_to_read" type="1" type_size="1" nature="1" inner="0" tooltip="" id="3" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="4" /><Input name="reset_Counter" type="1" type_size="1" nature="1" inner="0" tooltip="" id="5" /><Input name="check_all" type="1" type_size="1" nature="1" inner="0" tooltip="" id="6" /><Output name="onStopped" type="3" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="7" /><Output name="onNoMail" type="3" type_size="1" nature="2" inner="0" tooltip="" id="8" /><Output name="out_mail_to_read" type="3" type_size="1" nature="2" inner="0" tooltip="" id="9" /></Box><Box name="New messages" id="3" localization="8" tooltip="Pepper tells the user how many new messages they have and asks whether they would like to read those." x="309" y="25"><bitmap>media/images/box/interaction/say.png</bitmap><script language="4"><content><![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.tts = ALProxy('ALTextToSpeech')
        self.ttsStop = ALProxy('ALTextToSpeech', True) #Create another proxy as wait is blocking if audioout is remote

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.ttsStop.stop(id)
            except:
                pass
        while( self.bIsRunning ):
            time.sleep( 0.2 )

    def onInput_onStart(self, p):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
            sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.tts.post.say(str(sentence))
            self.ids.append(id)
            self.tts.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.onStopped() # activate output of the box
                self.bIsRunning = False

    def onInput_onStop(self):
        self.onUnload()]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when Diagram is loaded." id="1" /><Input name="onStart" type="3" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this Input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this Input." id="3" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when Box behavior is finished." id="4" /><Parameter name="Voice shaping (%)" inherits_from_parent="1" content_type="1" value="100" default_value="100" min="50" max="150" tooltip='Used to modify at runtime the voice feature (tone, speed). In a slighty&#x0A;different way than pitch and speed, it gives a kind of &quot;gender or age&#x0A;modification&quot; effect.&#x0A;&#x0A;For instance, a quite good male derivation of female voice can be&#x0A;obtained setting this parameter to 78%.&#x0A;&#x0A;Note: For a better effect, you can compensate this parameter with the&#x0A;speed parameter. For example, if you want to decrease by 20% the voice&#x0A;shaping, you will have to increase by 20% the speed to keep a constant&#x0A;average speed.' id="5" /><Parameter name="Speed (%)" inherits_from_parent="1" content_type="1" value="100" default_value="100" min="50" max="200" tooltip="Changes the speed of the voice.&#x0A;&#x0A;Note: For a better effect, you can compensate this parameter with the voice&#x0A;shaping parameter. For example, if you want to increase by 20% the speed, you&#x0A;will have to decrease by 20% the voice shaping to keep a constant average&#x0A;speed." id="6" /><Resource name="Speech" type="Lock" timeout="0" /></Box><Box name="Get mail" id="7" localization="8" tooltip="" x="687" y="126"><bitmap>media/images/box/box-python-script.png</bitmap><script language="4"><content><![CDATA[# -*- coding: utf-8 -*-
import imaplib
import email

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.isRunning = False

    def onLoad(self):
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        try :
          self.mail.logout()
          self.memory = None
        except :
          pass

    # Download all attachment files for a given email
    def downloadAttachmentsInEmail(self, m, emailid, savedir):
        resp, data = m.uid('fetch', emailid, "(BODY.PEEK[])")
        email_body = data[0][1]
        msg = email.message_from_string(email_body)
        self.logger.info(msg.get_content_maintype())
        if msg.get_content_maintype() != 'multipart':
            return ""

        partfiles = []
        for part in msg.walk():
            if part.get_content_maintype() != 'multipart' and part.get('Content-Disposition') is not None:
                if not os.path.exists(savedir):
                    os.mkdir(savedir)
                fullname = os.path.join(savedir, part.get_filename())
                fileobj = open(fullname, 'wb')
                content = part.get_payload(decode=True)
                fileobj.write(str(content))
                fileobj.close()
                partfiles.append(str(part.get_filename()))     # for caller to open
        return partfiles

    def mail_init(self):
        mail = imaplib.IMAP4_SSL('imap.gmail.com')

        # Get id and pass
        human_id = self.memory.getData("user_id")
        human_pass = self.memory.getData("user_pass")

        mail.login(human_id, human_pass)

        mail.list()
        mail.select('INBOX')

        return mail

    def onInput_onStart(self):

        mail = self.mail_init()

        # Get uid of chosen mail
        e_id = self.memory.getData("current_mail_id")
        # Get the chose mail
        _, data = mail.uid('fetch', e_id, '(RFC822)')

        for response_part in data:
            if isinstance(response_part, tuple):
                msg = email.message_from_string(response_part[1])
                #'subject' part of an email
                varSubject = msg['subject']
                #Decode subject to read non-acsii letters
                varSubject = email.Header.decode_header(varSubject)[0]
                varSubject = str(varSubject[0])
                #'From' part of an email
                varFrom = msg['from']
                #'Date' part of an email
                varDate = msg['date']
                # Check if email has text/plain attachment, skip it to get correct main body
                attachment_exist = "1"
                if msg.is_multipart():
                    #get attachment
                    attachment = self.downloadAttachmentsInEmail(mail, e_id,"/var/tmp")
                    self.logger.info(str(attachment))
                    if not attachment:
                        attachment_exist = "0"
                    else:
                        self.onAttachment(attachment)
                    for part in msg.walk():
                        ctype = part.get_content_type()
                        cdispo = str(part.get('Content-Disposition'))

                        # skip any text/plain (txt) attachments
                        if ctype == 'text/plain' and 'attachment' not in cdispo:
                            body = part.get_payload(decode=True)  # decode
                            break
                # not multipart - i.e. plain text, no attachments, keeping fingers crossed
                else:
                    body = msg.get_payload(decode=True)
                    attachment_exist = "0"
        #replace english date with finnish date
        dates = ['Mon', 'Tue', 'Wed', 'Thu', 'Fri', 'Sat', 'Sun']
        dates_fi = ['Maanantai', 'Tiistai', 'Keskiviikko', 'Torstai', 'Perjantai', 'Lauantai', 'Sunnuntai']

        day = (varDate.split(","))[0]
        for i in range(len(dates)):
                if day == dates[i]:
                    varDate = varDate.replace(day, dates_fi[i])
                    break

        #replace english month with finnish month
        months = ['Jan', 'Feb', 'Mar', 'Apr', 'May', 'Jun', 'Jul', 'Aug', 'Sep', 'Oct', 'Nov', 'Dec']
        #encode non-ascii characters
        kesa = u'Kesäkuuta'
        kesa = kesa.encode('utf-8')
        heina = u'Heinäkuuta'
        heina = heina.encode('utf-8')
        months_fi = ['Tammikuuta', 'Helmikuuta', 'Maaliskuuta', 'Huhtikuuta', 'Toukokuuta', kesa, heina, 'Elokuuta', 'Syyskuuta', 'Lokakuuta','Marraskuuta','Joulukuuta']
        mon = (varDate.split(" "))[2]
        for i in range(len(months)):
                if mon == months[i]:
                    varDate = varDate.replace(mon, months_fi[i])
                    break

        #Combine 'from', 'subject', 'date' and 'body' into one string to send to Say box, not including the year (rsplit 3) for the sake of... Well, people are not reading more than a-year-old emails.
        sender = varFrom.split("<")[0]

        # Only say the current message without pevious reply/conversation and forward info
        if "Forwarded message" in body:
            body_speak = str(body).split(">")[2]
        else:
            body_speak = str(body).split(">")[0]
            body_speak = body_speak.rsplit("\n",1)[0]
            #self.logger.info(body_speak)

        # Output to speak by Pepper
        my_mail = sender +"\n\\pau=500\\" + varDate.rsplit(" ",3)[0] +"\n\\pau=500\\" + varSubject + '\n\\pau=500\\' + str(body_speak)

        #remove the brackets around the sender email address
        varFrom = varFrom.replace('<', '[')
        varFrom = varFrom.replace('>', ']')

        # Output to show on tablet
        my_mail_show = varFrom +'\n'+ varDate.split("+")[0] +"\n" + varSubject + '\n' + str(str(body).replace("\n","<br>"))

        receiver_mail = varFrom.split('[')
        receiver_mail = receiver_mail[1].replace(']', '')
        self.logger.info(receiver_mail)
        # Add sender email for later reply
        self.memory.insertData("mailReceiverName",str(sender))
        self.memory.insertData("mailReceiver",str(receiver_mail))

        #log out
        mail.logout()
        self.onStopped(my_mail)
        self.onShow(my_mail_show)

        #check if attachment is empty
        self.logger.info(attachment_exist)
        self.onCheck(attachment_exist)
    def onInput_onStop(self):
        self.onUnload() #it is recommended to reuse the clean-up as the box is stopped
        self.onStopped() #activate the output of the box]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this input." id="3" /><Output name="onStopped" type="3" type_size="1" nature="1" inner="0" tooltip="Signal sent when box behavior is finished." id="4" /><Output name="onShow" type="3" type_size="1" nature="2" inner="0" tooltip="" id="5" /><Output name="onAttachment" type="3" type_size="1" nature="2" inner="0" tooltip="" id="6" /><Output name="onCheck" type="3" type_size="1" nature="2" inner="0" tooltip="" id="7" /></Box><Box name="Say email out loud" id="14" localization="8" tooltip="Gets the email as a string, including sender, header, date, and the message itself" x="930" y="8"><bitmap>media/images/box/interaction/say.png</bitmap><script language="4"><content><![CDATA[import time

class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)
        self.tts = ALProxy('ALTextToSpeech')
        self.ttsStop = ALProxy('ALTextToSpeech', True) #Create another proxy as wait is blocking if audioout is remote

    def onLoad(self):
        self.bIsRunning = False
        self.ids = []

    def onUnload(self):
        for id in self.ids:
            try:
                self.ttsStop.stop(id)
            except:
                pass
        while( self.bIsRunning ):
            time.sleep( 0.2 )

    def onInput_onStart(self, p):
        self.bIsRunning = True
        try:
            sentence = "\RSPD="+ str( self.getParameter("Speed (%)") ) + "\ "
            sentence += "\VCT="+ str( self.getParameter("Voice shaping (%)") ) + "\ "
            sentence += str(p)
            sentence +=  "\RST\ "
            id = self.tts.post.say(str(sentence))
            self.ids.append(id)
            self.tts.wait(id, 0)
        finally:
            try:
                self.ids.remove(id)
            except:
                pass
            if( self.ids == [] ):
                self.onStopped() # activate output of the box
                self.bIsRunning = False

    def onInput_onStop(self):
        self.onUnload()]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when Diagram is loaded." id="1" /><Input name="onStart" type="3" type_size="1" nature="2" inner="0" tooltip="Box behavior starts when a signal is received on this Input." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Box behavior stops when a signal is received on this Input." id="3" /><Output name="onStopped" type="1" type_size="1" nature="1" inner="0" tooltip="Signal sent when Box behavior is finished." id="4" /><Parameter name="Voice shaping (%)" inherits_from_parent="1" content_type="1" value="100" default_value="100" min="50" max="150" tooltip='Used to modify at runtime the voice feature (tone, speed). In a slighty&#x0A;different way than pitch and speed, it gives a kind of &quot;gender or age&#x0A;modification&quot; effect.&#x0A;&#x0A;For instance, a quite good male derivation of female voice can be&#x0A;obtained setting this parameter to 78%.&#x0A;&#x0A;Note: For a better effect, you can compensate this parameter with the&#x0A;speed parameter. For example, if you want to decrease by 20% the voice&#x0A;shaping, you will have to increase by 20% the speed to keep a constant&#x0A;average speed.' id="5" /><Parameter name="Speed (%)" inherits_from_parent="1" content_type="1" value="100" default_value="100" min="50" max="200" tooltip="Changes the speed of the voice.&#x0A;&#x0A;Note: For a better effect, you can compensate this parameter with the voice&#x0A;shaping parameter. For example, if you want to increase by 20% the speed, you&#x0A;will have to decrease by 20% the voice shaping to keep a constant average&#x0A;speed." id="6" /><Resource name="Speech" type="Lock" timeout="0" /></Box><Box name="Wait and loop to CheckMail" id="17" localization="8" tooltip="Wait a moment before sending a signal on the output. &#x0A;Can be stopped anytime. &#x0A;Stimulating the input again before output is activated restarts the waiting period.&#x0A;" x="1797" y="767"><bitmap>media/images/box/wait.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
    def __init__(self):
        GeneratedClass.__init__(self, False)

    def onLoad(self):
        self.waiting = None

    def onUnload(self):
        self.cancelWaiting()

    def triggerOutput(self):
        self.timerOutput()

    def cancelWaiting(self):
        if self.waiting:
            self.waiting.cancel()
        self.waiting = None

    def onInput_onStart(self):
        self.cancelWaiting()
        import qi
        self.waiting = qi.async(self.triggerOutput, delay=int(self.getParameter("Timeout (s)") * 1000 * 1000))

    def onInput_onStop(self):
        if self.getParameter("Trigger timerOutput if cancelled") and self.waiting and self.waiting.isRunning():
            self.timerOutput()
        self.onUnload()]]></content></script><Input name="onLoad" type="1" type_size="1" nature="0" inner="1" tooltip="Signal sent when diagram is loaded." id="1" /><Input name="onStart" type="1" type_size="1" nature="2" inner="0" tooltip="Start the Wait box with the configured timeout value." id="2" /><Input name="onStop" type="1" type_size="1" nature="3" inner="0" tooltip="Stop the wait and stimulate the output." id="3" /><Output name="timerOutput" type="1" type_size="1" nature="1" inner="0" tooltip="Send a bang once time set in parameters is elapsed, or if the box is stopped and the appropriate parameter is set." id="4" /><Parameter name="Timeout (s)" inherits_from_parent="0" content_type="2" value="2" default_value="1" min="0" max="5000" tooltip="Duration the box waits before stimulating the output." id="5" /><Parameter name="Trigger timerOutput if cancelled" inherits_from_parent="0" content_type="0" value="0" default_value="1" tooltip="If the box is currently waiting and cancelled, output will be stimulated." id="6" /></Box><Box name="Extract attachment" id="27" localization="8" tooltip="" x="2264" y="40"><bitmap>media/images/box/box-python-script.png</bitmap><script language="4"><content><![CDATA[class MyClass(GeneratedClass):
    #define global variable
    file_audio = []
    file_image = []
    file_text = []
    file_video = []
    file_unsupport = []
    list_attachment = []

    def __init__(self):
        GeneratedClass.__init__(self)

    def onLoad(self):
        self.frameManager = ALProxy('ALFrameManager')
        self.memory = ALProxy("ALMemory")

    def onUnload(self):
        self.frameManager = None
        self.memory = None

    def onInput_onStart(self,filename):
        import os
        import shutil
        os.chdir("/var/tmp")
        # Sort file based on type
        for file in filename:
            # Extract the file_name from the list
            file_name = (str(file)).replace("['","")
            file_name = (str(file_name)).replace("']","")

            if (file_name.lower()).endswith(('.txt', '.doc', '.docx')):
                #f = open(str(file_name))
                #t = f.read()
                #self.logger.info(str(t))
                self.file_text.append(file_name)
            elif (file_name.lower()).endswith(('.wav', '.mp3', '.ogg')):
                #self.onStopped(str(file_name))
                self.file_audio.append(file_name)
            elif (file_name.lower()).endswith(('.png', '.jpg', '.jpeg')):
                # move the file to html folder to open it
                path = "../html/attachment/image/" + str(file_name)
                des = os.path.join(self.frameManager.getBehaviorPath(self.behaviorId), str(path))
                source = os.path.join(os.path.dirname(os.path.realpath("__file__")), str(file_name))
                try:
                    shutil.copy(source,des) #this moves the file
                except Exception as e:
                    self.logger.error(e)

                # Checking path
                self.logger.info(str(des))
                self.logger.info(str(source))
                #self.onPic(str(file_name))
                self.file_image.append(file_name)
            elif (file_name.lower()).endswith('.mp4'):
                # move the file to html folder to open it
                path = "../html/attachment/video/" + str(file_name)
                des = os.path.join(self.frameManager.getBehaviorPath(self.behaviorId), str(path))
                source = os.path.join("/var/tmp", str(file_name))
                try:
                    shutil.copy(source,des) #this moves the file
                except Exception as e:
                    self.logger.error(e)

                self.file_video.append(file_name)
            else:
                #self.logger.info("File extension is not supported")
                self.file_unsupport.append(file_name)

        # create a list of number of attachment of each file and file name
        # store in ALMemory to get access to from JS
        # number of each type of attachment:
        # [0]:text; [1]:audio; [2]: image; [3]: unsupport

        self.list_attachment.extend((["text","audio","image","video","unsupported"],[str(len(self.file_text)),str(len(self.file_audio)),str(len(self.file_image)),str(len(self.file_video)),str(len(self.file_unsupport))]))

