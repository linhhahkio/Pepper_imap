        self.memory.insertData("list_video", self.file_video)
        self.memory.insertData("list_unsupport", self.file_unsupport)

        # If only 1 type of attachment, show directly the file
        # If 3 other types received no file
        if (self.list_attachment[1].count("0") == 4):
                for i,x in enumerate(self.list_attachment[1]):
                    if (x != "0"):
                        type = self.list_attachment[0][i]
                        self.onType(str(type))
        else:
            self.onShowList()

    def onInput_removeOldAttachment(self):
        import os
        for image in self.file_image:
            path = "../html/attachment/image/" + str(image)
            des = os.path.join(self.frameManager.getBehaviorPath(self.behaviorId), str(path))
